.slider,.slider swiper,.slider swiper image,.slider swiper navigator{
  display: block;
 width: 100%;
 height: 390rpx
}
.index-nav{
  display: flex;
  flex-wrap: wrap;
}
.index-nav .nav-item{
  width: calc(100% / 3);
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-bottom: 1rpx solid #ccc;
  font-size: 16px;
}
.index-nav .nav-item image{
  width: 80rpx;
  height: 80rpx
}

.index-enter image{
  width: 320rpx;
  height: 178rpx;
}

.index-enter{
  display: flex;
  padding: 20rpx 30rpx;
}
.enter-item{
  width: 50%;
  display: flex;
  justify-content: center
}

page{
  background-color: #fff
}